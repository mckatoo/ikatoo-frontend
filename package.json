{
  "name": "remix-template-remix",
  "private": true,
  "description": "Milton C. Katoo personal web page.",
  "license": "CC0-1.0",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.10.2",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icon-cloud": "4.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint": "^8.11.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.0.24",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16"
  }
}
